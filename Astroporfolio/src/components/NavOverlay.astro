                </svg>
                <button id="menu-close-btn" class="group">
                 
                 CLOSE
          
                   <span class="block max-w-0 group-hover:max-w-full transition-all duration-300 h-0.5 bg-gray-900"></span>
                </button>
            </div>
            
            <div class="flex items-center gap-4">
      
           
            
                 <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="w-6 h-6">
                    <path stroke-linecap="round" stroke-linejoin="round" d="M8.625 12a.375.375 0 11-.75 0 .375.375 0 01.75 0zm0 0H8.25m4.125 0a.375.375 0 11-.75 0 .375.375 0 01.75 0zm0 0H12m4.125 0a.375.375 0 11-.75 0 .375.375 0 01.75 0zm0 0h-.375M21 
 12c0 4.556-3.86 8.25-8.625 8.25S3.75 16.556 3.75 12c0-4.556 3.86-8.25 8.625-8.25S21 7.444 21 12z" />
                </svg>
       
                 <a href="#" class="group">
                    HIRE ME
                    <span class="block max-w-0 group-hover:max-w-full transition-all 
 duration-300 h-0.5 bg-gray-900"></span>
                </a>
            </div>
        </div>

     
        
        <div class="flex-1 flex flex-col justify-center">
            
            
            <div 
 class="mb-16 nav-link-animate" style="--link-delay: 100ms;"> 
                
                <span class="block w-16 h-0.5 bg-purple-600 mb-6"></span>
                 
                
                <div class="flex gap-5">
        
                    {socialIcons.map(social => (
                        <a href={social.href} class="text-gray-900 hover:text-purple-600 transition-colors" target="_blank" rel="noopener noreferrer">
    
                             <svg class="w-6 h-6" fill="currentColor" viewBox="0 0 24 24">
               
                                 <path d={social.icon}></path>
                            </svg>
        
                         </a>
                    ))}
 
                 </div>
            </div>

            
            <div class="flex flex-col gap-10">
        
                 <div class="flex flex-col gap-1 nav-link-animate sm:flex-row sm:items-baseline sm:gap-6" style="--link-delay: 200ms;">
              
                    <h2 class="font-bold text-5xl sm:text-6xl text-gray-900 hover:text-purple-600 transition-colors"><a href="/">Home</a></h2>
                    <p class="text-gray-700">Regreso a la pagina principal.</p>
                </div>
                
   
                 <div class="flex flex-col gap-1 nav-link-animate sm:flex-row sm:items-baseline sm:gap-6" style="--link-delay: 300ms;">
     
                   <h2 class="font-bold text-5xl sm:text-6xl text-gray-900 hover:text-purple-600 transition-colors"><a href="/proyectos">Proyectos</a></h2>
                    <p class="text-gray-700">Proyectos personales.</p>
                </div>
               
 
                <div class="flex flex-col gap-1 nav-link-animate sm:flex-row sm:items-baseline sm:gap-6" style="--link-delay: 400ms;">
   
                     <h2 class="font-bold text-5xl sm:text-6xl text-gray-900 hover:text-purple-600 transition-colors"><a href="/contacto">Contacto</a></h2>
                    <p class="text-gray-700">Hablame de lo que tienes planeado.</p>
                </div>
           
             </div>
           
  
        </div>
    </div> 
</div>